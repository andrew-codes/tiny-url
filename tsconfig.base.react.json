{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "types": ["cypress", "node"],
    "useDefineForClassFields": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
